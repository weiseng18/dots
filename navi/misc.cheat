% misc

# Check if a LaTeX package is installed
kpsewhich <package-name>.sty

# Install LaTeX package
sudo tlmgr install <package-name>

# Generate code dependency graph
madge --image graph.png <file-name>

$file-name: ls

# Push git tags
git push origin <tag-name>

$tag-name: git tag

# Delete remote branch
git push origin -d <branch-name>
