#!/usr/bin/env zsh

typeset -A local nav=(
  [d]="$DOTS"
  [s]="$REPOS/uni/Y1S4"
  [z]="$ZSH_DOTS"
  [r]="$REPOS"
  [m]="$REPOS/orbital/modtree"
  [t]="$DOTS/tmux/.config/tmux"
  [nv]="$NAVI_PATH"
  [cs]="$REPOS/nus-cheatsheets"
  [u]="$REPOS/uni"
)

for k dir in ${(kv)nav}; do
  [ ! -d $dir ] && echo "zsh nav: $dir doesn't exist" && continue
  alias 2$k="cd $dir"
done
unset nav k v
