# commit related
alias ga="git number add"
alias gcm="git commit -m"
alias gp="git push"

# diff/status
alias gdi="git number diff"
alias gds="git number diff --staged"
alias gst="git number"

function gl() {
    if [ "$1" != "" ]
    then
        git log --oneline -$1
    else
        git log --oneline -10
    fi
}
